<div id="Profile" class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <%= render 'shared/user_info' %>
    </section>
  </aside>
  <div class="col-md-8">
    <section class="timbre_list">
    <% if @user.timbres.any? %>
      <ul class="timbres">
        <%= render partial: 'timbres/timbre', collection: @timbres %>
      </ul>
    <% end %>
    </div>
  </div>
</div>